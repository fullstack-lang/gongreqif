{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "02 - run gongreqif --unmarshallFromCode=data/stage.go --marshallOnCommit=data/stage",
			"type": "shell",
			"options": {
				"cwd": "${workspaceFolder}/go/cmd/gongreqif"
			},
			"command": "go",
			"args": [
				"run",
				"main.go",
				"--unmarshallFromCode",
				"data/stage.go",
				"--marshallOnCommit",
				"data/stage.go"
			],
			"group": "build"
		},
		{
			"label": "03 - lsof -i tcp:8080 ",
			"type": "shell",
			"command": "lsof -i tcp:8080 ",
			"group": "build"
		},
		{
			"label": "03 - ng build",
			"type": "shell",
			"command": "cd ng-github.com-fullstack-lang-gongreqif; ng build",
			"group": "build",
			"problemMatcher": []
		},
		{
			"label": "04 - ng serve --host=127.0.0.1",
			"type": "shell",
			"command": "cd ng-github.com-fullstack-lang-gongreqif; ng serve --host=127.0.0.1",
			"group": "build",
			"problemMatcher": []
		},
		{
			"label": "01 - gong generate gongreqif",
			"type": "shell",
			"options": {
				"cwd": "${workspaceFolder}/go/models"
			},
			"command": "gong",
			"group": "build",
			"args": [
				"generate",
				"--level1",
				"--skipGoModCommands"
			]
		},
		{
			"label": "00 - go mod tidy",
			"type": "shell",
			"command": "go",
			"args": [
				"mod",
				"tidy"
			],
			"group": "build"
		},
		{
			"label": "00 - go mod vendor",
			"type": "shell",
			"group": "build",
			"command": "go",
			"args": [
				"mod",
				"vendor"
			],
		},
		{
			"label": "00 - go build",
			"type": "shell",
			"options": {
				"cwd": "${workspaceFolder}/go/cmd/gongreqif"
			},
			"command": "go",
			"args": [
				"build"
			],
			"group": "build"
		},
		{
			"label": "00 - go install",
			"type": "shell",
			"options": {
				"cwd": "${workspaceFolder}/go/cmd/gongreqif"
			},
			"command": "go",
			"args": [
				"install"
			],
			"group": "build"
		},
		{
			"label": "02 - run gongreqif upload",
			"type": "shell",
			"options": {
				"cwd": "${workspaceFolder}/go/cmd/gongreqif"
			},
			"command": "go",
			"args": [
				"run",
				".",
			],
			"group": "build"
		},
	]
}